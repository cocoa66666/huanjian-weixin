<view><view class="page-body"><view class="page-section page-section-gap"><map style="width:100%;height:100vh;" id="myMap" show-location latitude="{{latitude}}" longitude="{{longitude}}" markers="{{markers}}" scale="{{scale}}" data-event-opts="{{[['touchMove',[['mask',['$event']]]],['touchEnd',[['getData',['$event']]]],['tap',[['mask',['$event']]]]]}}" onMarkerTap="showPowerDetail" onCalloutTap="showPowerDetail" onTouchMove="__e" onTouchEnd="__e" onTap="__e"></map></view></view><view class="search"><image class="serach_img" src="../../static/img/sousuo3x.png"></image><input class="search_ipt" v-model="{{inputValue}}" placeholder="请输入你想要去的地点" placeholder-class="search_pla" value="" data-event-opts="{{[['tap',[['getInput',['$event']]]],['input',[['onKeyInput',['$event']]]]]}}" onTap="__e" onInput="__e"/></view><block a:if="{{inputValue!=''&&mask1==1}}"><view class="search_content"><block a:for="{{selectRes}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['moveToSelect',['$0','$1'],[[['selectRes','',index,'location.lat']],[['selectRes','',index,'location.lng']]]]]]]}}" class="search_content_word" onTap="__e">{{item.title}}</view></block></view></block><block a:if="{{egCode==''||egCode==undefined||egCode==null}}"><image class="search_scan" src="../../static/img/saoma23x.png" data-event-opts="{{[['tap',[['scan',['$event']]]]]}}" onTap="__e"></image></block><block a:if="{{egCode!=''&&egCode!=undefined&&egCode!=null}}"><image class="search_scan" src="../../static/img/saoma23x.png" data-event-opts="{{[['tap',[['warn',['$event']]]]]}}" onTap="__e"></image></block><image class="search_img" src="../../static/img/daiyuan3x.png" data-event-opts="{{[['tap',[['moveToNow',['$event']]]]]}}" onTap="__e"></image></view>